{
    "extends": "airbnb",
    "globals": {
},
"rules": {
  "no-unused-vars": ["error", { "argsIgnorePattern": "next"  }],
  "max-len": [0, { "code": 100, "ignoreStrings": true  }],
  "no-plusplus": [0],
  "indent": ["warn", 2],
  "no-warning-comments": ["error", {"terms": ["fixme"]}],
  "no-use-before-define": [0],
  "no-tab": [0],
  "prefer-destructuring": [0]
},
"overrides": [
    {
        "files": ["Test/*-test.js"],
        "rules": {
            "no-unused-expressions": "off"
        }
    }
]
}
